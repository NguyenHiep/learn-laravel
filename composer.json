{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.2",
        "laravelcollective/html": "^6.0",
        "watson/active": "^4.0.0",
        "intervention/image": "^2.4",
        "guzzlehttp/guzzle": "^6.3",
        "doctrine/dbal": "^2.5",
        "unisharp/laravel-filemanager": "^2.0.1",
        "fideloper/proxy": "^4.0",
        "laravel/tinker": "^2.0",
        "niklasravnsborg/laravel-pdf": "^4.0",
        "milon/barcode": "^6.0",
        "yajra/laravel-datatables-oracle": "^9.9",
        "yajra/laravel-datatables-buttons": "^4.9",
        "prettus/l5-repository": "^2.6",
        "league/fractal": "^0.18.0",
        "yajra/laravel-datatables-fractal": "^1.5",
        "prettus/laravel-validation": "^1.2",
        "spatie/laravel-permission": "^3.11",
        "laravel/helpers": "^1.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "^2.0",
        "barryvdh/laravel-debugbar": "^3.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "helper": {

        },
        "psr-4": {
            "App\\": "app/",
            "NMH\\WSDelivery\\": "packages/NMH/wsdelivery/src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
